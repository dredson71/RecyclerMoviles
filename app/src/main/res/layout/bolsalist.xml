<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:background="@drawable/card_bag"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/item_LastBolsa"
            android:paddingBottom="20dp"

        >


    <TextView
        android:layout_marginTop="20dp"
        android:layout_marginLeft="50dp"
        android:id="@+id/txtBolsa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="@string/app_name"
        android:textColor="@color/title_text_color"
         />


    <TextView
        android:id="@+id/txtCreadoFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif-black"
        android:layout_alignStart="@+id/txtBolsa"
        android:layout_alignLeft="@+id/txtBolsa"
        android:text="@string/app_name"
        android:layout_below="@+id/txtBolsa"
        android:textColor="@color/title_text_color" />
    <TextView
        android:id="@+id/txtRecoojoFecha"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtBolsa"
        android:layout_marginTop="20dp"
        android:layout_alignLeft="@+id/txtBolsa"
        android:fontFamily="sans-serif-black"
        android:text="@string/app_name"
        android:layout_below="@+id/txtCreadoFecha"
        android:textColor="@color/title_text_color" />


    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>